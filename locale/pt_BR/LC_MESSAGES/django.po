#: pos/templates/account/password_change.html:6
#: pos/templates/account/password_change.html:9
#: pos/templates/account/password_change.html:14
#: pos/templates/account/password_reset_from_key.html:5
#: pos/templates/account/password_reset_from_key.html:8
#: pos/templates/account/password_reset_from_key_done.html:4
#: pos/templates/account/password_reset_from_key_done.html:7
msgid "Change Password"
msgstr "Alterar Senha"

#: pos/templates/account/password_reset.html:7
#: pos/templates/account/password_reset.html:11
#: pos/templates/account/password_reset_done.html:6
#: pos/templates/account/password_reset_done.html:9
msgid "Password Reset"
msgstr "Redefinição de senha"

#: pos/templates/account/password_reset.html:16
msgid ""
"Forgotten your password? Enter your e-mail address below, and we'll send you "
"an e-mail allowing you to reset it."
msgstr "Esqueceu sua senha? Digite seu endereço de e-mail abaixo e enviaremos um e-mail permitindo que você o redefina."

#: pos/templates/account/password_reset.html:21
msgid "Reset My Password"
msgstr "Redefinir minha senha"

#: pos/templates/account/password_reset.html:24
msgid "Please contact us if you have any trouble resetting your password."
msgstr "Entre em contato conosco se tiver algum problema para redefinir sua senha."

#: pos/templates/account/password_reset_done.html:15
msgid ""
"We have sent you an e-mail. Please contact us if you do not receive it "
"within a few minutes."
msgstr "Enviamos um e-mail para você. Entre em contato conosco se você não recebê-lo dentro de alguns minutos."

#: pos/templates/account/password_reset_from_key.html:8
msgid "Bad Token"
msgstr "Token Inválido"

#: pos/templates/account/password_reset_from_key.html:12
#, python-format
msgid ""
"The password reset link was invalid, possibly because it has already been "
"used.  Please request a <a href=\"%(passwd_reset_url)s\">new password reset</"
"a>."
msgstr "O link de redefinição de senha era inválido, possivelmente porque já foi usado. "
"<a href=\"%(passwd_reset_url)s\">Solicite uma nova redefinição de senha</a>."

#: pos/templates/account/password_reset_from_key.html:18
msgid "change password"
msgstr "alterar senha"

#: pos/templates/account/password_reset_from_key.html:21
#: pos/templates/account/password_reset_from_key_done.html:8
msgid "Your password is now changed."
msgstr "Sua senha agora foi alterada."

#: pos/templates/account/password_set.html:6
#: pos/templates/account/password_set.html:9
#: pos/templates/account/password_set.html:14
msgid "Set Password"
msgstr "Definir Senha"

#: pos/templates/account/signup.html:6
msgid "Signup"
msgstr "Cadastro"

#: pos/templates/account/signup.html:9
#: pos/templates/account/signup.html:19
#: pos/templates/base.html:67
msgid "Sign Up"
msgstr "Cadastro"

#: pos/templates/account/signup.html:11
#, python-format
msgid ""
"Already have an account? Then please <a href=\"%(login_url)s\">sign in</a>."
msgstr "já tem uma conta? Então, por favor, faça <a href=\"%(login_url)s\">login</a>."

#: pos/templates/account/signup_closed.html:5
#: pos/templates/account/signup_closed.html:8
msgid "Sign Up Closed"
msgstr "Inscrições encerradas"

#: pos/templates/account/signup_closed.html:10
msgid "We are sorry, but the sign up is currently closed."
msgstr "Lamentamos, mas as inscrições estão encerradas no momento."

#: pos/templates/account/verification_sent.html:5
#: pos/templates/account/verification_sent.html:8
#: pos/templates/account/verified_email_required.html:5
#: pos/templates/account/verified_email_required.html:8
msgid "Verify Your E-mail Address"
msgstr "Verifique seu endereço de e-mail"

#: pos/templates/account/verification_sent.html:10
msgid ""
"We have sent an e-mail to you for verification. Follow the link provided to "
"finalize the signup process. Please contact us if you do not receive it "
"within a few minutes."
msgstr "Enviamos um e-mail para você para verificação. Siga o link fornecido para finalizar o processo de inscrição. Entre em contato conosco se você não recebê-lo dentro de alguns minutos."

#: pos/templates/account/verified_email_required.html:12
msgid ""
"This part of the site requires us to verify that\n"
"you are who you claim to be. For this purpose, we require that you\n"
"verify ownership of your e-mail address. "
msgstr "Esta parte do site exige que verifiquemos se você é quem afirma ser.\n"
"Para esse fim, exigimos que você verifique a propriedade\n"
"do seu endereço de e-mail."

#: pos/templates/account/verified_email_required.html:16
msgid ""
"We have sent an e-mail to you for\n"
"verification. Please click on the link inside this e-mail. Please\n"
"contact us if you do not receive it within a few minutes."
msgstr "Enviamos um e-mail para você para verificação.\n"
"Por favor, clique no link dentro deste e-mail.\n"
"Entre em contato conosco se você não recebê-lo dentro de alguns minutos."

#: pos/templates/account/verified_email_required.html:20
#, python-format
msgid ""
"<strong>Note:</strong> you can still <a href=\"%(email_url)s\">change your e-"
"mail address</a>."
msgstr "<strong>Nota</strong>: você ainda pode <a href=\"%(email_url)s\">alterar seu endereço de e-mail</a>."

